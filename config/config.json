{
  "smartgrazer": {
    "logging": {
      "level": 20,
      "logfile": "run.log"
    },
    "directories": {
      "runconfigs": "/../../config/targets/"
    },
    "imps": {
      "smithy": {
        "generate": {
          "amount": 1
        },
        "generator": "smarty",
        "elements": {
          "usages": "smarty/elements.json",
          "lifes": "smarty/elements.life.json"
        }
      },
      "simpy": {
        "stringgenerator": {
          "length": {
            "min": 5,
            "max": 35
          },
          "specialchars": [
            "EXCLAMATIONMARK",
            "QUOTE:s",
            "QUOTE:d",
            "QUOTE:b",
            "QUOTE:n",
            "PERCENT",
            "OPENROUNDBRACKET",
            "CLOSEROUNDBRACKET",
            "PLUS",
            "MINUS",
            "DOT",
            "SLASH",
            "COLON",
            "OPENPOINTEDBRACKET",
            "CLOSEPOINTEDBRACKET",
            "EQUAL",
            "OPENSQUAREBRACKET",
            "CLOSESQUAREBRACKET",
            "POWER",
            "UNDERLINE",
            "BACKSLASH",
            "OPENCURLYBRACKET",
            "CLOSECURLYBRACKET"
          ]
        },
        "checks": {
          "keywords": true,
          "chars": true,
          "specialchars": true,
          "max_get_length": false,
          "jsfuck": true
        }
      },
      "dharma": {
        "script": "/../dharma/dharma/dharma.py",
        "grammar": "/config/smartgrazer.dg"
      },
      "smarty": {
        "attacks": "smarty/attacks.json",
        "outbreaks": "smarty/outbreaks.json",
        "grammars": "smarty/grammars.json",
        "context": "both",
        "# context_options": [
          "both",
          "html",
          "javascript"
        ]
      },
      "jsfuck": {
        "# Not used at all": "at the moment",
        "enabled": true,
        "script": "jsfuck3.py"
      },
      "webber": {
        "cleanup": false,
        "# timelimit in minutes": "amount will be multiplied with 60 to get minutes",
        "timelimit": 5
      },
      "sandy": {
        "response": {
          "savedir": "results/sandy/"
        },
        "proxies": {},
        "proxies_example": {
          "http": "http://127.0.0.1"
        }
      },
      "annelysa": {
        "webdriver": "Chrome",
        "#webdriver_options": [
          "Firefox",
          "Chrome"
        ]
      }
    }
  }
}